<div class="sta roll attribute">
  <div class="dice-roll">
    <div class="dice-result">
      <div class="dice-tooltip">
        <section class="tooltip-part">
          <div class="dice">
            <ol class="dice-rolls">{{ challengeRollList item.rolls.challenge.roll }}</ol>
          </div>
        </section>
      </div>
      {{{item.rolls.challenge.effects}}}
      <h4 class="dice-total">{{{item.rolls.challenge.success}}}</h4>
    </div>
  </div>
  <div class="reroll-result challenge">
    <span>{{ localize 'sta.roll.rerollresults' }}</span>
    <input id="speakerId" type="hidden" value="{{item.speaker.id}}">
  </div>
</div>
